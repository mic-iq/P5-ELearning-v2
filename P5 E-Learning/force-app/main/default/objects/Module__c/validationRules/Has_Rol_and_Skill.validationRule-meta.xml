<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Has_Rol_and_Skill</fullName>
    <active>true</active>
    <errorConditionFormula>AND( Active__c ,(OR(ISBLANK(Rol__c), ISPICKVAL(Skill__c,&apos;&apos; ))))</errorConditionFormula>
    <errorDisplayField>Active__c</errorDisplayField>
    <errorMessage>Module can&apos;t be activated without having a skill and a roll</errorMessage>
</ValidationRule>
